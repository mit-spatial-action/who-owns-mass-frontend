{
	"name": "svelte-map-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prebuild": "node generate-redirects.js",
		"css-build": "sass --load-path=node_modules/ src/lib/styles/style.scss src/lib/styles/style.css",
		"start": "npm run css-build -- --watch",
