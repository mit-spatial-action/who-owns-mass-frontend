		"@sveltejs/kit": "^2.7.7",
		"sass": "^1.80.6",
		"svelte": "^5.1.10",
		"vite": "^5.4.10"
	},
	"type": "module",
	"dependencies": {
		"@deck.gl/core": "^9.0.34",
		"@deck.gl/layers": "^9.0.34",
		"@deck.gl/mapbox": "^9.0.34",
		"@mapbox/mapbox-gl-geocoder": "^5.0.3",
		"@rollup/plugin-json": "^6.1.0",
		"@turf/turf": "^7.1.0",
		"bulma": "^1.0.2",
		"dotenv": "^16.4.5",
		"mapbox-gl": "^3.7.0",
		"mdsvex": "^0.12.3",
		"netlify-cli": "^17.37.2",
		"svelte-device-info": "^1.0.6",
		"svelte-routing": "^2.13.0",
		"turf": "^3.0.14"
