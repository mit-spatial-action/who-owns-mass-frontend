{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "paths": {
        "$lib/*": ["./src/lib/*"],
        "$routes/*": ["./src/routes/*"],
        "$types": ["./.svelte-kit/types/src/routes/$types"]
      }
    },
    "include": ["src/**/*.ts", "src/**/*.js", "src/**/*.svelte", ".svelte-kit/types/src/*"],
    "exclude": ["node_modules"]
  }
  